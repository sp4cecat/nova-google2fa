{
    "name": "sp4cecat/nova-google2fa",
    "description": "Forked from the lifeonscreen package, makes changes to the authenticate blade",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0",
        "bacon/bacon-qr-code": "^2.0",
        "pragmarx/recovery": "^0.1.0",
        "pragmarx/google2fa-laravel": "^0.2.0"
    },
    "autoload": {
        "psr-4": {
            "Sp4cecat\\Google2fa\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Sp4cecat\\Google2fa\\ToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
